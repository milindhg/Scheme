#lang racket
(require C311/pmatch)
(require C311/trace)

(define last-non-zero
 (lambda (ls) 
  (call/cc
   (lambda (k)
    (letrec
     ((lnz
      (lambda (ls)
       (cond
        ((null? ls) '())
        ((zero? (car ls)) (k (lnz (cdr ls))))  ;;Try building a list and whenever a zero is occuring , try to break the loop and print the numbers after zero.
        (else (cons (car ls) (lnz (cdr ls)))))
	  )))
    (lnz ls))))))



